{
  "name": "express_start",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "test",
    "server:dev": "NODE_ENV=development nodemon server.js",
    "server": "NODE_ENV=production nodemon server.js",
    "sequelize": "npx ./node_modules/sequelize-cli/lib/sequelize",
    "db:migrate": "npx ./node_modules/sequelize-cli/lib/sequelize db:migrate --config ./db_config/config.js",
    "db:rollback": "npx ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all --config ./db_config/config.js",
    "db:seed": "npx ./node_modules/sequelize-cli/lib/sequelize db:seed:all --config ./db_config/config.js",
    "build": "npm install --prefix ./front-end && npm run build --prefix ./front-end",
    "db:init": "docker run -d --name spasibo-db -e POSTGRES_PASSWORD=password123 -p 54320:5432 postgres",
    "db:start": "docker start spasibo-db",
    "db:bootstrap": "node bootstrap.js",
    "start": "npm run build && NODE_ENV=production npm run db:migrate && NODE_ENV=production npm run db:seed && npm run server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.7",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.2.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "date-fns": "^2.5.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "lodash": "^4.17.15",
    "lodash.omit": "^4.5.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "sequelize": "^5.18.4",
    "sequelize-cli": "^5.5.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.0.0"
  }
}
